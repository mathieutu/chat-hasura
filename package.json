{
  "name": "chat-hasura",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint --ext ts,js*,vue,gql . --fix",
    "hasura": "cd ./hasura && hasura",
    "hasura:console": "yarn hasura console",
    "hasura:dev": "yarn hasura:start && yarn hasura:console",
    "hasura:seeds": "yarn hasura seeds",
    "hasura:migrate": "yarn hasura migrate",
    "hasura:start": "docker-compose up -d",
    "schema": "graphql get-schema",
    "codegen": "graphql-codegen -w",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@vue/apollo-composable": "^4.0.0-alpha.10",
    "@vue/apollo-option": "^4.0.0-alpha.7",
    "apollo-boost": "^0.4.9",
    "core-js": "^3.8.2",
    "date-fns": "^2.16.1",
    "ramda": "^0.27.1",
    "ramda-adjunct": "^2.30.0",
    "tailwindcss": "^1.8.11",
    "vue": "^3.0.0-0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-vue-apollo": "^2.3.1",
    "@tailwindcss/ui": "^0.6.2",
    "@types/ramda": "^0.27.34",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@vue/cli-plugin-babel": "~4.5.10",
    "@vue/cli-plugin-eslint": "~4.5.10",
    "@vue/cli-plugin-router": "~4.5.10",
    "@vue/cli-plugin-typescript": "~4.5.10",
    "@vue/cli-service": "~4.5.10",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.18.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^7.4.1",
    "graphql": "^15.4.0",
    "graphql-cli": "^4.1.0",
    "graphql-tag": "^2.9.0",
    "hasura-cli": "^1.3.3",
    "patch-package": "^6.2.2",
    "typescript": "~4.1.3"
  }
}
